#include<stdio.h>
#include<stdlib.h>

int main(int argc, char *argv[]){
	int i,j,cont,ordem=12;
	char op;
	double a, M[ordem][ordem];
	
	scanf("%c",&op);
	
	
	for(i = 0; i < ordem; i++){
		for(j = 0; j < ordem; j++){
			scanf("%lf", &M[i][j]);
		}
	}

	a = 0;
	cont = 0;
	for(i = 0; i < ordem; i++){
		for(j = 0; j < ordem;j++){
			if(j > 1){
				a += M[i][j];
				cont++;
			}
			
		}
	}
		
	if(op = 'S'){
		printf("%.1lf\n",a);
	}
	else if(op = 'M'){
		double media = a/cont;
		printf("%.1lf\n", media);
	}
			
}
